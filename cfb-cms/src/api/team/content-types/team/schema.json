{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team",
    "description": "Dental clinic team members and staff"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "Name": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "required": true
    },
    "Image": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Function": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Licence": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "bio": {
      "type": "richtext",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "specializations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::dental-service.dental-service"
    },
    "displayOrder": {
      "type": "integer",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "email": {
      "type": "email",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      }
    },
    "Active": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": true
    }
  }
}
